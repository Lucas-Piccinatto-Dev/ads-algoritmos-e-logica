programa {
  funcao inicio() {
    //Declaração de Variaveis
    inteiro n1
    inteiro n2

    //Entrada de Dados
    escreva("digite o primeiro numero:")
    leia(n1)
    escreva("digite o segundo numero:")
    leia(n2)

    //Saida de Dados
    escreva("O primeiro numero digitado é:", n1)
    escreva("\nO segundo numero digitado é:", n2)
  }
}
